{"user": "Launch Puppeteer browser instance", "assistant": "const puppeteer = require('puppeteer'); const browser = await puppeteer.launch();"}
{"user": "Create new page in Puppeteer", "assistant": "const page = await browser.newPage();"}
{"user": "Navigate to URL with Puppeteer", "assistant": "await page.goto('https://example.com');"}
{"user": "Get page title", "assistant": "await page.title()"}
{"user": "Get current URL", "assistant": "page.url()"}
{"user": "Set viewport size", "assistant": "await page.setViewport({width: 1920, height: 1080});"}
{"user": "Find element with CSS selector", "assistant": "await page.$('h1')"}
{"user": "Find multiple elements", "assistant": "await page.$$('a')"}
{"user": "Click element by selector", "assistant": "await page.click('button#submit')"}
{"user": "Type text into input field", "assistant": "await page.type('input[name=\"username\"]', 'user123')"}
{"user": "Press keyboard key", "assistant": "await page.keyboard.press('Enter')"}
{"user": "Evaluate JavaScript in page context", "assistant": "await page.evaluate(() => document.title)"}
{"user": "Extract element text content", "assistant": "await page.$eval('h1', el => el.textContent)"}
{"user": "Extract data from multiple elements", "assistant": "await page.$$eval('a', links => links.map(l => l.href))"}
