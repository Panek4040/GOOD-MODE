{"instruction": "Search for the word 'password' in all files under /var/www", "input": {"path": "/var/www", "pattern": "password"}, "output": {"tool": "text_search_basic", "action": "grep -r -n 'password' /var/www", "result": "/var/www/config.php:15:$password = 'secret123';\n/var/www/auth.php:42:if ($password == $hash) {"}}
{"instruction": "Find all files containing 'ERROR' in /var/log directory", "input": {"path": "/var/log", "pattern": "ERROR"}, "output": {"tool": "text_search_basic", "action": "grep -r -l 'ERROR' /var/log | sort", "result": "/var/log/apache2/error.log\n/var/log/mysql/error.log\n/var/log/syslog"}}
{"instruction": "Count how many times 'failed' appears in log files", "input": {"path": "/var/log", "pattern": "failed"}, "output": {"tool": "text_search_basic", "action": "grep -r -n 'failed' /var/log | wc -l", "result": "342"}}
{"instruction": "Search for 'database' ignoring case in /etc", "input": {"path": "/etc", "pattern": "database"}, "output": {"tool": "text_search_case_insensitive", "action": "grep -r -i -n 'database' /etc", "result": "/etc/app/config.ini:23:Database=production\n/etc/mysql/my.cnf:45:# DATABASE SETTINGS"}}
{"instruction": "Find files with 'connection refused' in any case variation", "input": {"path": "/var/log", "pattern": "connection refused"}, "output": {"tool": "text_search_case_insensitive", "action": "grep -r -i -l 'connection refused' /var/log | sort", "result": "/var/log/apache2/error.log\n/var/log/syslog"}}
{"instruction": "Search for IP addresses matching pattern in logs", "input": {"path": "/var/log", "pattern": "192\\.168\\.[0-9]+\\.[0-9]+"}, "output": {"tool": "text_search_case_insensitive", "action": "grep -r -i -E '192\\.168\\.[0-9]+\\.[0-9]+' /var/log", "result": "/var/log/auth.log:Failed password from 192.168.1.100\n/var/log/apache2/access.log:192.168.1.105 - - [18/Nov/2025:10:30:42]"}}
{"instruction": "Find 'TODO' comments in PHP files only", "input": {"path": "/var/www", "pattern": "TODO", "extension": "php"}, "output": {"tool": "text_search_by_filetype", "action": "find /var/www -type f -name '*.php' -exec grep -n 'TODO' {} +", "result": "/var/www/index.php:42:// TODO: Add validation\n/var/www/admin.php:89:// TODO: Implement caching"}}
{"instruction": "Search for 'import' statements in Python files", "input": {"path": "/opt/app", "pattern": "^import", "extension": "py"}, "output": {"tool": "text_search_by_filetype", "action": "find /opt/app -type f -name '*.py' -exec grep -n '^import' {} \\; | sort", "result": "/opt/app/main.py:1:import os\n/opt/app/main.py:2:import sys\n/opt/app/utils.py:1:import re"}}
{"instruction": "Count occurrences of 'function' in JavaScript files", "input": {"path": "/var/www/js", "pattern": "function", "extension": "js"}, "output": {"tool": "text_search_by_filetype", "action": "find /var/www/js -type f -name '*.js' | xargs grep -c 'function' 2>/dev/null | grep -v ':0$'", "result": "/var/www/js/app.js:34\n/var/www/js/utils.js:12\n/var/www/js/main.js:8"}}
{"instruction": "Search in config files but exclude vendor directory", "input": {"path": "/var/www", "pattern": "api_key", "exclude_dir": "vendor"}, "output": {"tool": "text_search_with_exclusions", "action": "grep -r -n --exclude-dir=vendor 'api_key' /var/www", "result": "/var/www/config.php:23:$api_key = 'sk_live_abc123';\n/var/www/settings.php:45:define('API_KEY', 'xyz789');"}}
{"instruction": "Find error messages excluding node_modules and cache", "input": {"path": "/var/www", "pattern": "error", "exclude_dir": "node_modules"}, "output": {"tool": "text_search_with_exclusions", "action": "grep -r -n --exclude-dir='node_modules' --exclude-dir='.*' 'error' /var/www | wc -l", "result": "156"}}
{"instruction": "Search source code excluding minified files", "input": {"path": "/var/www", "pattern": "console.log", "exclude_dir": "vendor"}, "output": {"tool": "text_search_with_exclusions", "action": "grep -r -l --exclude-dir='vendor' --exclude='*.min.js' --exclude='*.log' 'console.log' /var/www", "result": "/var/www/js/app.js\n/var/www/js/debug.js"}}
{"instruction": "Search for API endpoints in text files excluding logs", "input": {"path": "/var/www", "pattern": "/api/v[0-9]+", "exclude_dir": "logs"}, "output": {"tool": "text_search_with_exclusions", "action": "find /var/www -type d -name 'logs' -prune -o -type f -exec grep -l '/api/v[0-9]+' {} \\; 2>/dev/null", "result": "/var/www/api/routes.php\n/var/www/config/api.yaml"}}
{"instruction": "Search for password in binary files", "input": {"path": "/usr/local/bin", "pattern": "password"}, "output": {"tool": "text_search_binary_and_fixed", "action": "grep -r -a -n 'password' /usr/local/bin", "result": "/usr/local/bin/app:4523:Binary file matches"}}
{"instruction": "Search for exact string with special characters", "input": {"path": "/var/www", "pattern": "p@$$w0rd"}, "output": {"tool": "text_search_binary_and_fixed", "action": "grep -r -F -n 'p@$$w0rd' /var/www", "result": "/var/www/config.php:15:$password = 'p@$$w0rd';"}}
{"instruction": "Extract strings from binary and search", "input": {"path": "/usr/bin/myapp", "pattern": "api_key"}, "output": {"tool": "text_search_binary_and_fixed", "action": "strings /usr/bin/myapp | grep 'api_key'", "result": "api_key=sk_live_abc123\nset_api_key_function"}}
{"instruction": "Search excluding binary files", "input": {"path": "/var/www", "pattern": "secret"}, "output": {"tool": "text_search_binary_and_fixed", "action": "grep -r -I -l 'secret' /var/www | sort", "result": "/var/www/config.php\n/var/www/settings.ini"}}
{"instruction": "Find files containing either 'error' or 'warning'", "input": {"path": "/var/log", "pattern1": "error", "pattern2": "warning"}, "output": {"tool": "text_search_multipattern", "action": "grep -r -E 'error|warning' /var/log", "result": "/var/log/syslog:Nov 18 10:30 error in module\n/var/log/syslog:Nov 18 10:31 warning: disk space low"}}
{"instruction": "Find files containing both 'password' and 'admin'", "input": {"path": "/var/www", "pattern1": "password", "pattern2": "admin"}, "output": {"tool": "text_search_multipattern", "action": "grep -r -l 'password' /var/www | xargs grep -l 'admin'", "result": "/var/www/admin/config.php\n/var/www/includes/auth.php"}}
{"instruction": "Search for failed login with username patterns", "input": {"path": "/var/log", "pattern1": "failed", "pattern2": "root|admin"}, "output": {"tool": "text_search_multipattern", "action": "grep -r -E 'failed' /var/log | grep -E 'root|admin'", "result": "/var/log/auth.log:Failed password for root from 192.168.1.100\n/var/log/auth.log:Failed password for admin from 203.0.113.45"}}
{"instruction": "Count files by error type", "input": {"path": "/var/log", "pattern1": "ERROR", "pattern2": "WARNING"}, "output": {"tool": "text_search_multipattern", "action": "grep -r -E 'ERROR|WARNING' /var/log | awk -F: '{print $1}' | sort | uniq -c | sort -rn", "result": "    127 /var/log/apache2/error.log\n     89 /var/log/syslog\n     42 /var/log/mysql/error.log"}}
{"instruction": "Show file sizes for files containing credentials", "input": {"path": "/var/www", "pattern": "password"}, "output": {"tool": "text_search_with_metadata", "action": "grep -r -l 'password' /var/www | xargs ls -lh", "result": "-rw-r--r-- 1 www-data www-data 4.2K Nov 18 10:30 /var/www/config.php\n-rw-r--r-- 1 www-data www-data  12K Nov 18 09:15 /var/www/auth.php"}}
{"instruction": "Find when files with errors were last modified", "input": {"path": "/var/log", "pattern": "ERROR"}, "output": {"tool": "text_search_with_metadata", "action": "grep -r -l 'ERROR' /var/log | xargs stat -c '%n|%s|%y' | sort -t'|' -k2 -rn", "result": "/var/log/syslog|524288|2025-11-18 14:30:42.000000000\n/var/log/apache2/error.log|102400|2025-11-18 14:25:15.000000000"}}
{"instruction": "Show size and modification time for config files", "input": {"path": "/etc", "pattern": "database"}, "output": {"tool": "text_search_with_metadata", "action": "find /etc -type f -exec grep -l 'database' {} \\; -exec stat -c '%n %s bytes, modified: %y' {} \\;", "result": "/etc/mysql/my.cnf 4301 bytes, modified: 2025-11-15 08:30:22.123456789\n/etc/app/config.ini 2048 bytes, modified: 2025-11-18 10:15:33.987654321"}}
{"instruction": "Count matches and show file sizes", "input": {"path": "/var/www", "pattern": "TODO"}, "output": {"tool": "text_search_with_metadata", "action": "grep -r -c 'TODO' /var/www | grep -v ':0$' | while IFS=: read -r file count; do size=$(stat -c %s \"$file\" 2>/dev/null); echo \"$file|$count matches|$size bytes\"; done | sort -t'|' -k2 -rn", "result": "/var/www/legacy/old.php|15 matches|8192 bytes\n/var/www/api/users.php|8 matches|4096 bytes"}}
{"instruction": "Search for function definitions in code", "input": {"path": "/var/www", "pattern": "^function\\s+\\w+"}, "output": {"tool": "text_search_case_insensitive", "action": "grep -r -i -E '^function\\s+\\w+' /var/www", "result": "/var/www/lib/utils.php:function sanitize_input($data)\n/var/www/includes/db.php:function connect_database()"}}
{"instruction": "Find SQL queries in source code", "input": {"path": "/var/www", "pattern": "SELECT.*FROM", "extension": "php"}, "output": {"tool": "text_search_by_filetype", "action": "find /var/www -type f -name '*.php' -exec grep -n 'SELECT.*FROM' {} +", "result": "/var/www/api/users.php:45:$sql = 'SELECT * FROM users WHERE id = $id';\n/var/www/reports.php:89:$query = 'SELECT name, email FROM customers';"}}
